{
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ],
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "node": "8.10"
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "8.10"
          },
          "modules": "commonjs"
        }]
      ]
    }
  }
}
